{
  "manifest_version": 2,

  "name": "WalkMe Checker Plugin",
  "description": "This extension will check if WalkMe is enabled",
  "version": "1.0",

  "background": {
    "scripts": ["js/background.js"]
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "permissions": [
    "tabs", "<all_urls>"
  ]
}