{
  "manifest_version": 2,

  "name": "WalkMe Checker Plugin",
  "description": "This extension will check if WalkMe is enabled",
  "version": "1.0",

  "background": {
    "scripts": ["js/background.js"]
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "web_accessible_resources": [
    "Frames/(walkme.com/)/Scripts/settings.txt"
    // "style/double-rainbow.css",
    // "script/double-rainbow.js",
    // "script/main.js",
    // "templates/*"
  ],

 "content_scripts": [
    {
    "all_frames": false,
    "matches": ["<all_urls>"],
    "exclude_matches": [],
      "js": [
        "js/jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ]

}